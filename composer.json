{
    "name":         "aequasi/psr-6-doctrine-bridge",
    "type":         "library",
    "description":  "PSR-6 Doctrine bridge",
    "keywords":     [
        "php",
        "cache",
        "redis",
        "memcached",
        "psr-6",
        "doctrine"
    ],
    "homepage":     "https://github.com/aequasi/psr-6-doctrine-bridge",
    "license":      "MIT",
    "authors":      [
        {
            "name":     "Aaron Scherer",
            "email":    "aequasi@gmail.com",
            "homepage": "https://github.com/aequasi"
        }
    ],
    "support":      {
        "email": "aequasi@gmail.com"
    },
    "require":      {
        "php":            "^5.5|^7.0",
        "doctrine/cache": "~1.3",
        "psr/cache":      "dev-master"
    },
    "repositories": [
        {
            "type": "git",
            "url":  "https://github.com/php-fig/cache.git"
        }
    ],
    "require-dev":  {
        "phpunit/phpunit": "^5.1|^4.0"
    },
    "autoload":     {
        "psr-4": {
            "Aequasi\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aequasi\\Cache\\Tests\\": "tests/"
        }
    }
}
